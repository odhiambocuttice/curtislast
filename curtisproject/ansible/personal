 server {
    listen 8000;
    server_name odhiambo.tk;

    location = /favicon.ico { access_log off; log_not_found off; }

    location /static/ {
            root /home/cuttice/dockerproject/docker/app-version;
    }

    location / {
            include proxy_params;
            proxy_pass http://unix:/home/cuttice/dockerproject/docker/app-versionpersonalapp.sock;
    }
}




